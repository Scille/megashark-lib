{
  "name": "megashark-lib",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "files": [
    "dist"
  ],
  "types": "./dist/typings.d.ts",
  "scripts": {
    "dev": "vite",
    "web:open": "vite --open",
    "web:release": "npm run lint && vite build",
    "build": "vite build",
    "build:local": "npm link && vite build --watch",
    "lint": "eslint . --max-warnings=0 && vue-tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "test:unit": "vitest run --dom",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write"
  },
  "dependencies": {
    "@ionic/vue": "^7.0.0",
    "@rive-app/canvas": "^2.15.0",
    "ionicons": "^7.0.0",
    "luxon": "^3.4.4",
    "vue": "^3.3.0",
    "vue-i18n": "^9.6.5"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@ionic/cli": "^7.2.0",
    "@ionic/vue-router": "^7.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/luxon": "^3.3.4",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/typescript": "^1.8.20",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "@zxcvbn-ts/language-fr": "^3.0.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-vue-scoped-css": "^2.5.1",
    "happy-dom": "^14.7.1",
    "prettier": "3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "start-server-and-test": "^2.0.3",
    "terser": "^5.4.0",
    "tslib": "^2.4.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.8.3",
    "vite-plugin-lib-inject-css": "^2.0.1",
    "vitest": "^1.5.1",
    "vue-router": "^4.2.0",
    "vue-tsc": "^1.0.24"
  },
  "description": "MegaShark-Lib"
}
